		<EGDC_ICON a="255" r="45" g="70" b="135" />
		<EGDC_ICON_HIGH_LIGHT a="255" r="70" g="190" b="240" />
		
		<Caption tbardistancex="4" tbardistancey="8" />
		<WindowButton width="24" />
		<CheckBoxColor a="255" r="33" g="161" b="33" />


    	</Global>
  
	<Button texture="button.png">
